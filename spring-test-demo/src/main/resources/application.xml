<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	   xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
	<bean id="circleA" class="com.xintong.spring.entity.circulation.CircleA">
		<property name="circleB" ref="circleB"/>
	</bean>
	<bean id="circleB" class="com.xintong.spring.entity.circulation.CircleB">
		<property name="circleA" ref="circleA"/>
	</bean>
	<bean id="singleton" class="com.xintong.spring.entity.circulation.Singleton">
		<property name="name" value="zsk"/>
		<property name="age" value="27"/>
	</bean>
</beans>